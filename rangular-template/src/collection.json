{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "component-template": {
      "description": "Where the parameters reside",
      "factory": "./component-template/index#componentTemplate",
      "schema": "./component-template/schema.json"
    },
    "module-template": {
      "description": "Where the parameters reside",
      "factory": "./module-template/index#moduleTemplate",
      "schema": "./module-template/schema.json"
    },
    "routing-template": {
      "description": "Scaffold routing in function of components and route links",
      "factory": "./routing-template/index#routingTemplate",
      "schema": "./routing-template/schema.json"
    },
    "service-template": {
      "description": "Scaffold service in function of methods and data handled by methods",
      "factory": "./service-template/index#serviceTemplate",
      "schema": "./service-template/schema.json"
    },
    "scaffold-template": {
      "description": "all the rest",
      "factory": "./scaffold-template/index#scaffoldTemplate",
      "schema": "./scaffold-template/schema.json"
    }
  }
}
